axios.post('', {\r\n          params : {\r\n            projectIdx : this.$store.state.setting.projectIdx\r\n          }\r\n        }).then(res =>{\r\n          if(res) {\r\n            alert(\"정상적으로 수정되었습니다..\");\r\n          }\r\n        }).catch(() =>{\r\n          alert(\"수정에 실패했습니다.\");\r\n        });\r\n      }else{\r\n        alert(\"취소했음\")\r\n      } \r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\ndiv{\r\n  margin-top : 10%;\r\n  display :flex;\r\n  justify-content : flex-end;\r\n  align-items : stretch;\r\n  flex-direction: row;\r\n  width : 60vw;\r\n  /* height : 100px; */\r\n}\r\n\r\n.modify-btn{\r\n  background-color : #FF8906;\r\n  width : 100px;\r\n  height : 30px;\r\n  margin : 20px;\r\n  border-radius : 5px;\r\n}\r\n\r\n.end-btn{\r\n  background-color : #FF8906;\r\n  width : 180px;\r\n  height : 30px;\r\n  margin : 20px;\r\n  border-radius : 5px;\r\n}\r\n\r\n.delete-btn{\r\n  background-color : #FF5C5C;\r\n  width : 180px;\r\n  height : 30px;\r\n  margin : 20px;\r\n  border-radius : 5px;\r\n}\r\n</style>"]}]}